{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-close",
    "window:allow-minimize",
    "window:allow-start-dragging",
    "window:allow-toggle-maximize",
    "window:allow-internal-toggle-maximize",
    "webview:default",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default",
    "store:default",
    "fs:default",
    {
      "identifier": "fs:allow-create",
      "allow": [{ "path": "$APPDATA/*" }]
    }
  ]
}
